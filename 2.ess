start
new i number
set i 1

loop rows (10 > i)
    new j number
    set j 1
    
    loop cols (10 > j)
        new product number
        set product 0
        new k number
        set k 1
        
        loop calc (i > k )
            set product product + j
            set k k + 1
        end calc
        
        log string product
        delete product
        delete k
        set j j + 1
    end cols
    
    delete j
    set i i + 1
end rows
finish
